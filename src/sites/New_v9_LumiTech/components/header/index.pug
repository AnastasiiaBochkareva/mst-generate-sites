mixin Header(param)
    - 
        const headerLinks = [
            {
              href: 'index.html',
              text: 'Main',
            },
            {
              href: 'catalog.html',
              text: 'Catalog',
            },
            {
              href: 'index.html#callback-form',
              text: 'Contact',
            },
            {
              href: 'services.html',
              text: 'Services',
            }
            
        ]

    header.header
        .container.header__container
            .header__logo
                +Link({
                    href: 'index.html',
                    text: 'LumiTech'
                })
            ul.header__list
                each link in headerLinks
                    li.header__list-item
                        +Link({
                            href: link.href,
                            text: link.text
                        })
            +Nav-mob({})
            
                            